  "scripts": {
    "mkdir": "mkdir -p build",
    "clean": "rm -f build/date.js && rm -rf build",

    "build": "npm run clean && npm run mkdir && npm run build:html && npm run build:css && npm run build:js && cp date.js build/",
    "build:html": "npm run clean:html && cp index.html build/",
    "build:css": "npm run clean:normalize-css && npm run clean:app-css && cp normalize.css build/ && cp app.css build/",
    "clean:normalize-css": "rm -f build/normalize.css",
    "clean:app-css": "rm -f build/app.css",
    "clean:html": "rm -f buid/index.html",
    "build:js": "npm run clean:js && webpack",
    "clean:js": "rm -f build/$npm_package_name.$npm_package_version.js build/$npm_package_name.$npm_package_version.js.map",
    "git-checkout": "git checkout gh-pages",
    "git-add": "git add .",
    "git-commit": "git commit -am 'deploy'",
    "git-push": "git push origin gh-pages",
    "deploy": "npm run git-checkout && npm run build && npm run git-add && npm run git-commit && npm run git-push",
    "test": "mocha --compilers js:babel-register tests/**/*.js*"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
